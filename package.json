{
  "name": "geekpress",
  "version": "1.0.0",
  "repository": {},
  "license": "MIT",
  "description": "Minimalistic Elixir Blog Engine",
  "private": true,
  "scripts": {
    "link-config": "ln -fs $BLOG_PATH/config ./",
