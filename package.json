    "build-production": "gulp build-production --production"
  },
  "devDependencies": {
    "chalk": "^1.0.0",
    "del": "^2.1.0",
    "gulp": "^3.8.11",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "~2.6.0",
    "gulp-replace": "~0.5.3",
    "gulp-sass": "~2.3.1",
    "gulp-size": "~2.1.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "~1.5.1",
    "vinyl-paths": "^2.1.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "font-awesome": "^4.4.0",
    "jquery": "^2.2.4"
  }
}
